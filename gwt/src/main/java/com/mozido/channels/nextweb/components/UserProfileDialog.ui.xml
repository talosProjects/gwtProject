<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:mb="urn:import:com.mozido.channels.nextweb.components.form.bootstrap"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
    <g:HTMLPanel>
        <b:Form>
            <b:FieldSet>
                <b:Legend ui:field="userName"/>

                <b:FormGroup>
                    <b:FormLabel for="tenantName">Tenant Name</b:FormLabel>
                    <mb:MzBootstrapTextBox type="TEXT" ui:field="tenantName" placeholder="Tenant Name"/>
                </b:FormGroup>
                <b:FormGroup>
                    <b:FormLabel for="roleName">Role Name</b:FormLabel>
                    <mb:MzBootstrapTextBox type="TEXT" ui:field="roleName" placeholder="Role Name"/>
                </b:FormGroup>                
                <b:FormGroup>
                    <b:FormLabel for="personType">Person Type</b:FormLabel>
                    <mb:MzBootstrapTextBox type="TEXT" ui:field="personType" placeholder="Person Type"/>
                </b:FormGroup>
                <b:FormGroup>
                    <b:FormLabel for="timeZone">Time Zone</b:FormLabel>
                    <mb:MzBootstrapTextBox type="TEXT" ui:field="timeZone" placeholder="Time Zone"/>
                </b:FormGroup>
                <b:FormGroup>
                    <b:FormLabel for="timeZoneOffset">Time Zone Offset</b:FormLabel>
                    <mb:MzBootstrapTextBox type="TEXT" ui:field="timeZoneOffset" placeholder="Time Zone Offset"/>
                </b:FormGroup>
                
            </b:FieldSet>
        </b:Form>
    </g:HTMLPanel>
</ui:UiBinder>