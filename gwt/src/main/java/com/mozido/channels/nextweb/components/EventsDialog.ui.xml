<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:com.mozido.channels.nextweb.components.form.bootstrap"

             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
    <g:HTMLPanel>
        Hello from events dialog!
        <g:InlineLabel ui:field="eventMessage"/>

        <div>
            <b:MzBootstrapButton ui:field="event1Button">
                <ui:msg description="Event A">Event A</ui:msg>
            </b:MzBootstrapButton>

            <b:MzBootstrapButton ui:field="event2Button">
                <ui:msg description="Event B">Event B</ui:msg>
            </b:MzBootstrapButton>

            <b:MzBootstrapButton ui:field="closeButton">
                <ui:msg description="Cancel">Cancel</ui:msg>
            </b:MzBootstrapButton>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>